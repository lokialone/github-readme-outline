{
    "content_scripts": [
      {
        "matches": ["https://github.com/*", "https://git.souche-inc.com/*"],
        "css": ["src/index.css"],
        "js": ["lib/index.js"]
      }
    ],
    "browser_action": {
      "default_icon": "icon.png"
   },
    "description": "github readme outline",
    "manifest_version": 2,
    "name": "github-outline",
    "permissions": [ "storage", "tabs", "contextMenus", "http://*/*", "https://*/*" ],
    "version": "1.0"
 }
 